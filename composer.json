{
    "name": "swoft-fork/apollo",
    "type": "library",
    "version": "v2.0.11",
    "keywords": [
        "php",
        "swoole",
        "swoft"
    ],
    "description": "micro-service framework base on swoole",
    "license": "Apache-2.0",
    "require": {
        "php": ">8.0",
        "swoft-fork/stdlib": "~2.0.11"
    },
    "autoload": {
        "psr-4": {
            "Swoft\\Apollo\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "autoload-dev": {
        "psr-4": {
            "SwoftTest\\Apollo\\Unit\\": "test/unit/",
            "SwoftTest\\Apollo\\Testing\\": "test/testing/"
        }
    },
    "repositories": {
    },
    "scripts": {
        "test": "./vendor/bin/phpunit -c phpunit.xml"
    }
}
